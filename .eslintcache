\n  ],\n};\n\nexport default (state = defaultState, action: LanguageActionTypes) => {\n  switch (action.type) {\n    case CHANGE_LANGUAGE:\n      i18n.changeLanguage(action.payload); // 这样处理是不标准的，有副作用\n      return { ...state, language: action.payload };\n    case ADD_LANGUAGE:\n      return {\n        ...state,\n        languageList: [...state.languageList, action.payload],\n      };\n    default:\n      return state;\n  }\n};\n","/Users/zhangjiaming/Desktop/react-shop/src/redux/language/languageActions.ts",[],"/Users/zhangjiaming/Desktop/react-shop/src/redux/hooks.ts",[],"/Users/zhangjiaming/Desktop/react-shop/src/redux/recommendProducts/recommendProductsReducer.ts",["279"],"import {\n  FETCH_RECOMMEND_PRODUCTS_FAIL,\n  FETCH_RECOMMEND_PRODUCTS_START,\n  FETCH_RECOMMEND_PRODUCTS_SUCCESS,\n  RecommendProductAction,\n} from './recommendProductsActions';\n\ninterface RecommendProductsState {\n  productList: any[];\n  loading: boolean;\n  error: string | null;\n}\n\nconst defaultState: RecommendProductsState = {\n  loading: true,\n  error: null,\n  productList: [],\n};\n\nexport default (state = defaultState, action: RecommendProductAction) => {\n  switch (action.type) {\n    case FETCH_RECOMMEND_PRODUCTS_START:\n      return { ...state, loading: true };\n    case FETCH_RECOMMEND_PRODUCTS_SUCCESS:\n      return { ...state, loading: false, productList: action.payload };\n    case FETCH_RECOMMEND_PRODUCTS_FAIL:\n      return { ...state, loading: false, error: action.payload };\n    default:\n      return state;\n  }\n};\n","/Users/zhangjiaming/Desktop/react-shop/src/redux/recommendProducts/recommendProductsActions.ts",[],"/Users/zhangjiaming/Desktop/react-shop/src/redux/middlewares/actionLog.ts",[],"/Users/zhangjiaming/Desktop/react-shop/src/components/productIntro/index.ts",[],"/Users/zhangjiaming/Desktop/react-shop/src/components/productIntro/ProductIntro.tsx",[],"/Users/zhangjiaming/Desktop/react-shop/src/components/productComments/index.ts",[],"/Users/zhangjiaming/D